<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <div>
        <input name="foto" type="file" id="file"/>
        <input id="request_img" type="button" value="Enviar"/>
    </div>
    <script>
        $('#request_img').on('click', async() => {
            const file = $('#file')[0].files;
            const formData = new FormData();
            for(const img of file){
                formData.append('file[]', img);
            }
            $.ajax({
                url: '/img_upload',
                dataType: 'json',
                method: 'POST',
                data: formData,
                contentType: false,
                cache: false,
                processData: false
            });
        })
    </script>
</body>
</html>